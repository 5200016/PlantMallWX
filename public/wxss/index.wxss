page {
    background: #f1f2fb;
}

.weui-loadmore {
    width: 65%;
    margin: 50% auto;
    display: none;
    line-height: 1.6em;
    font-size: 28rpx;
    text-align: center;
}

.weui-loadmore__tips {
    display: inline-block;
    vertical-align: middle;
}

.weui-loadmore_line {
    border-top: 1px solid #E5E5E5;
    margin-top: 50%;
}

.weui-loadmore__tips_in-line {
    padding: 0 .55em;
    background-color: #f1f2fb;
    color: #999999;
    margin-top: -55rpx;
}

.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    box-sizing: border-box;
}

.dbflex {
    display: flex;
}

.top_bor {
    position: fixed;
    top: 0;
    left: 0;
    height: 0;
    z-index: 9;
    border-bottom: 1rpx solid #e5e5e5;
    width: 100%;
}

.toast_screen {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: #fff;
    opacity: 0.2;
    overflow: hidden;
    z-index: 1000;
    color: #fff;
}

.toast_screen_box {
    width: 100%;
    overflow: hidden;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 2000;
    background: #fff;
}

.toast_screen_view {
    bottom: 200rpx;
    font-size: 12pt;
    margin-bottom: 200rpx;
    width: 100%;
    height: fit-content;
    display: flex;
    flex-direction: row;
    justify-content: center;
}

.toast_screen_title_view {
    height: fit-content;
    width: fit-content;
    padding-bottom: 10px;
    padding-top: 10px;
    padding-left: 20px;
    padding-right: 20px;
    border: 1px #A9A9A9 solid;
    border-radius: 10px;
    color: #ffffff;
    background-color: #A9A9A9;
    text-align: center;
}

.nodata_more {
    font-size: 26rpx;
    color: #888;
    width: 100%;
    height: 90rpx;
    line-height: 90rpx;
    float: left;
    text-align: center;
}

.db {
    display: block;
}

.fl {
    float: left;
}

.fr {
    float: right;
}

.clearfix:after {
    content: ".";
    display: block;
    height: 0;
    visibility: hidden;
    clear: both;
}

.page_hd {
    width: 100%;
    line-height: 80rpx;
    font-size: 14px;
    text-align: center;
    float: left;
}

.page_hd_tips {
    display: inline-block;
    vertical-align: middle;
}

.page_hd-loading {
    margin: 0 5px;
    width: 20px;
    height: 20px;
    display: inline-block;
    vertical-align: middle;
    animation: weuiLoading 1s steps(12,end) infinite;
    background: transparent url(data:image/svg+xml;base64,) no-repeat;
    background-size: 100%;
}

@-webkit-keyframes weuiLoading {
    0% {
        transform: rotate3d(0,0,1,0deg);
    }

    100% {
        transform: rotate3d(0,0,1,360deg);
    }
}

@keyframes weuiLoading {
    0% {
        transform: rotate3d(0,0,1,0deg);
    }

    100% {
        transform: rotate3d(0,0,1,360deg);
    }
}

.flex {
    display: -webkit-box!important;
    display: -webkit-flex!important;
    display: -ms-flexbox!important;
    display: flex!important;
}

.flex-v-center {
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
}

.flex-extend {
    -webkit-box-flex: 1;
    flex: 1;
}

button::after {
    border: none;
}

.dn {
    display: none;
}