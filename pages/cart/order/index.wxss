@import "../../../public/wxss/index.wxss";

.container {
    padding-bottom: 140rpx;
}

.detail_top {
    width: 100%;
    float: left;
    border-top: 1rpx solid #e5e5e5;
    padding-bottom: 40rpx;
    background: #fff;
    font-size: 28rpx;
    background-image: url(https://w.cxzg.com/Public2/wechat_applet/images/address_bg.png);
    background-position: bottom;
    background-size: 60rpx 10rpx;
    background-repeat: repeat-x;
}

.detail_top navigator {
    width: 100%;
    display: flex;
}

.choose_address {
    width: 95%;
    float: left;
}

.choose_address span {
    display: block;
    margin-top: 73rpx;
    margin-bottom: 40rpx;
}

.choose_address span i {
    font-style: normal;
    margin-left: 31rpx;
    font-size: 34rpx;
    color: #ff344b;
}

.choose_btn {
    margin-top: 10%;
}

.choose_btn image {
    width: 12rpx;
    height: 24rpx;
}

.order_id {
    border-bottom: 1rpx solid #e5e5e5;
    color: #353535;
    margin-left: 30rpx;
    padding: 27rpx;
}

.order_id text {
    float: right;
    color: #888;
}

.order_id text.current {
    color: #ff344b;
}

.sh_name {
    font-size: 34rpx;
    color: #000;
    margin: 35rpx 30rpx 20rpx 30rpx;
}

.sh_name text {
    float: right;
}

.sh_address {
    margin: 0 30rpx;
    color: #353535;
    font-size: 28rpx;
    line-height: 35rpx;
}

.order_cont {
    background: #fff;
    border-top: 1rpx solid #e5e5e5;
    border-bottom: 1rpx solid #e5e5e5;
    float: left;
    margin-bottom: 10rpx;
    width: -webkit-calc(100% - 20rpx);
    width: calc(100% - 20rpx);
}

.order_cont .title {
    font-size: 28rpx;
    color: #000;
    width: 100%;
    height: 78rpx;
    line-height: 78rpx;
    float: left;
}

.order_cont .title image {
    width: 30rpx;
    height: 40rpx;
    margin: 19rpx 12rpx 19rpx 10rpx;
    float: left;
}

.order_cont .title .tit {
    float: left;
    width: 75%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.order_cont .title text {
    float: right;
    font-size: 24rpx;
    color: #888;
}

.order_cont .title text.current {
    color: #ff344b;
}

.order_cont .cont_t {
    padding-left: 10rpx;
    display: block;
    border-bottom: 1rpx solid #E5E5E5;
    margin-top: 20rpx;
    margin-bottom: 10rpx;
}

.order_cont .cont_t image {
    width: 160rpx;
    height: 160rpx;
    margin: 10rpx;
    margin-right: 15rpx;
    float: left;
}

.shop_check {
    height: 40rpx;
    float: left;
    margin-top: 20rpx;
    margin-right: 10rpx;
}

.order_cont .cont_t .con {
    width: auto;
    padding: 5rpx 0 0 0;
}

.order_cont .cont_t .con .tit {
    font-size: 28rpx;
    line-height: 30rpx;
    color: #000;
    margin-bottom: 23rpx;
    margin-top: 15rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    width: 70%;
}

.order_cont .cont_t .con .txt {
    font-size: 24rpx;
    line-height: 30rpx;
    color: #353535;
    height: 65rpx;
    text-overflow: -o-ellipsis-lastline;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.order_cont .cont_t .con .price {
    font-size: 28rpx;
    color: #ff344b;
    padding-right: 25rpx;
}

.order_cont .cont_t .con text {
    font-size: 26rpx;
    float: right;
    color: #353535;
    line-height: 26rpx;
    padding: 5rpx 0;
}

.order_cont .con_b {
    text-align: right;
    margin-top: 10rpx;
    width: 100%;
    float: left;
    border-bottom: 1rpx solid #e5e5e5;
}

.order_cont .con_b navigator {
    width: 136rpx;
    height: 60rpx;
    border-radius: 5rpx;
    border: 1rpx solid #e5e5e5;
    text-align: center;
    line-height: 60rpx;
    font-size: 28rpx;
    color: #888;
    margin-right: 20rpx;
    margin-bottom: 20rpx;
    float: right;
}

.order_cont .con_b .setbtn {
    border: 1rpx solid #ff8a00;
    color: #ff8a00;
}

.order_cont .con_b .pay_btn {
    border: 1rpx solid #ff8a00;
    background: #ff8a00;
    color: #fff;
}

.order_cont .con_b .pay_btn:hover {
    border: 1rpx solid #ff8a00;
    background: #ff8a00;
    color: #fff;
}

.detail_xx {
    width: 100%;
    float: left;
    padding-left: 20rpx;
    padding-top: 20rpx;
}

.detail_xx .txt {
    font-size: 28rpx;
    color: #000;
    line-height: 35rpx;
    margin-bottom: 20rpx;
}

.detail_xx .txt text {
    float: right;
    margin-right: 30rpx;
}

.detail_xx .txt span {
    float: right;
    margin-right: 30rpx;
    color: #ff344b;
}

.liuyan {
    color: #000;
    font-size: 28rpx;
    border-top: 1rpx solid #e5e5e5;
    display: flex;
    width: 100%;
    padding-left: 20rpx;
}

.liuyan text {
    margin: 36rpx 0;
    width: 70rpx;
    font-size: 28rpx;
    line-height: 28rpx;
}

.liuyan input {
    margin-top: 25rpx;
    -webkit-box-flex: 1;
    flex: 1;
    padding: 0 20rpx 0;
    font-size: 28rpx;
}

.dil_xx {
    border-bottom: 1rpx solid #e5e5e5;
    width: 100%;
    font-size: 28rpx;
    height: 80rpx;
    line-height: 80rpx;
}

.dil_xx .tbt {
    float: left;
    vertical-align: middle;
}

.dil_xx.no {
    border-bottom: none;
}

.dil_xx .tat {
    float: right;
    color: #ff344b;
}

.dil_xx .tat text {
    color: #000;
    padding-right: 20rpx;
}

.dil_xx .tbt text {
    display: block;
    float: left;
    height: 80rpx;
    line-height: 80rpx;
}

.detail_bot {
    position: fixed;
    background: #fff;
    left: 0;
    bottom: 0;
    text-align: right;
    height: 98rpx;
    line-height: 98rpx;
    border-top: 1rpx solid #e5e5e5;
    width: 100%;
    font-size: 34rpx;
    z-index: 666;
}

.detail_bot text {
    color: #ff344b;
    line-height: 98rpx;
}

.detail_bot .pay {
    width: 280rpx;
    float: right;
    height: 98rpx;
    line-height: 98rpx;
    background: #ff8a00;
    color: #fff;
    text-align: center;
    margin-left: 20rpx;
}

.detail_bot .pay button {
    width: 280rpx;
    float: right;
    height: 98rpx;
    line-height: 98rpx;
    background: #ff8a00;
    color: #fff;
    text-align: center;
    margin-left: 20rpx;
}

.clear {
    clear: both;
}

.plea_youhui {
    border-top: 1rpx solid #e5e5e5;
    height: 100rpx;
    line-height: 100rpx;
    padding-left: 20rpx;
}

.plea_youhui text {
    width: 50%;
    float: left;
    font-size: 28rpx;
}

.plea_youhui .choose_btn {
    float: right;
    margin-right: 20rpx;
    margin-top: 0;
}

.coupon {
    height: 100rpx;
    line-height: 100rpx;
    width: -webkit-calc(50% - 48rpx);
    width: clac(50% - 48rpx);
    float: left;
    font-size: 28rpx;
    color: #888;
    padding-right: 16rpx;
    text-align: right;
}

.coupon-null {
    line-height: 120rpx;
    font-size: 36rpx;
    color: #b2b2b2;
    text-align: center;
}

.coupon-wrap {
    overflow: hidden;
}

.groupSum {
    line-height: 80rpx;
    font-size: 28rpx;
    text-align: right;
    padding-right: 30rpx;
}

.groupSum text {
    color: #ff344b;
}

.set_hui {
    width: 100%;
    position: fixed;
    height: 100%;
    bottom: 0;
    left: 0;
    right: 0;
    z-index: 999;
}

.black {
    background: #000;
    opacity: 0.6;
    width: 100%;
    bottom: 0;
    left: 0;
    right: 0;
    position: fixed;
    height: 100%;
}

.hui_list {
    min-height: 600rpx;
    max-height: 600rpx;
    z-index: 9999;
    position: fixed;
    width: 100%;
    bottom: 0;
    overflow-y: auto;
    background-color: #fff;
    padding-top: 30rpx;
}

.hui_title {
    height: 60rpx;
    line-height: 60rpx;
}

.hui_title .dian {
    margin-left: 40rpx;
}

.you_hui {
    font-size: 28rpx;
    height: 100rpx;
    line-height: 100rpx;
    margin-left: 40rpx;
    border-bottom: 1rpx solid #e5e5e5;
}

.you_hui text {
    float: left;
    width: 80%;
}

.hui_img {
    width: 40rpx;
    height: 40rpx;
    float: right;
    margin: 30rpx 40rpx 0 0;
}

.dian {
    display: block;
    width: 10rpx;
    height: 10rpx;
    background: #000;
    border-radius: 50%;
    float: left;
    margin: 24rpx 4rpx 0 10rpx;
}

.icon-wrap {
    position: absolute;
    right: 0;
    top: 0;
    width: 120rpx;
    height: 70rpx;
    text-align: center;
    padding-top: 20rpx;
}

.all_checkyou {
    float: right;
    height: 20rpx;
    margin: 20rpx 40rpx 0 0;
}

.he {
    width: 60%;
    float: left;
}

.not_mo {
    width: 50%;
    height: 98rpx;
    line-height: 98rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    float: right;
    text-align: center;
}

.line {
    height: 1rpx;
    background: #e5e5e5;
    margin-left: -100rpx;
}

.phclass {
    font-size: 28rpx;
    height: 28rpx;
}

.coupon-bg {
    opacity: 0;
    position: fixed;
    top: 0;
    right: 0;
    left: 0;
    height: 100%;
}
