    margin-left: auto;
    margin-right: auto;
    padding-left: 0;
    padding-right: 0;
    box-sizing: border-box;
    text-align: left;
    text-decoration: none;
    border-radius: 0;
    line-height: 1.5;
    -webkit-tap-highlight-color: transparent;
    overflow: hidden;
    color: #000000;
    background-color: #fff;
}

button.but_ui::after {
    content: " ";
    height: 0;
    border: none;
    border-radius: 0;
}

