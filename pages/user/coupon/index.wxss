@import "../../../public/wxss/index.wxss";

view,text {
    box-sizing: border-box;
}

.tab-nav {
    height: 84rpx;
    font-size: 30rpx;
    background: #fff;
    display: flex;
    line-height: 81rpx;
    border-bottom: 1rpx solid #e5e5e5;
    width: 100%;
    position: fixed;
    top: 1rpx;
    left: 0;
    z-index: 9999;
}

.tab-nav .txt {
    color: #353535;
    display: block;
    -webkit-box-flex: 1;
    flex: 1;
    text-align: center;
    width: 33.333%;
    position: relative;
}

.tab-nav text {
    display: inline-block;
    line-height: 82rpx;
}

.tab-nav text.active {
    color: #ff8a00;
    border-bottom: 4rpx solid #ff8a00;
    line-height: 80rpx;
    padding: 0 3%;
}

.list-wrap {
    width: 100%;
    padding: 102rpx 25rpx 20rpx 25rpx;
}

.list-item {
    width: 100%;
    margin-bottom: 20rpx;
}

.list-up {
    padding: 0 30rpx;
    border-top-left-radius: 10rpx;
    border-top-right-radius: 10rpx;
    background: #fff;
    box-shadow: 0 0 8rpx rgba(0,0,0,0.1);
}

.title {
    font-size: 30rpx;
    padding: 25rpx 0 10rpx 0;
}

.intro {
    font-size: 24rpx;
    color: #353535;
    line-height: 34rpx;
}

.deadline {
    font-size: 24rpx;
    color: #888;
    padding: 10rpx 0 10rpx 0;
}

.line-wrap {
    height: 38rpx;
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: stretch;
    -ms-flex-align: stretch;
    align-items: stretch;
    background: #fff;
    position: relative;
    z-index: 999;
}

.line-left {
    width: 35rpx;
    background: url("data:image/png;base64,") no-repeat left center;
    background-size: auto 100%;
}

.line {
    height: 20rpx;
    border-bottom: 1rpx solid #e5e5e5;
    -webkit-box-flex: 1;
    flex: 1;
}

.line-right {
    width: 35rpx;
    background: url("data:image/png;base64,") no-repeat right center;
    background-size: auto 100%;
}

.shop-wrap {
    padding: 10rpx 40rpx 20rpx 30rpx;
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: stretch;
    -ms-flex-align: stretch;
    align-items: stretch;
    background: #fff;
    border-bottom-right-radius: 10rpx;
    border-bottom-left-radius: 10rpx;
    box-shadow: 0 0 15rpx rgba(0,0,0,0.1);
}

.shop {
    width: 70%;
    -webkit-box-flex: 0;
    -ms-flex-positive: 0;
    flex-grow: 0;
}

.name {
    font-size: 28rpx;
    line-height: 38rpx;
    height: 38rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    margin-bottom: 4rpx;
    padding-left: 40rpx;
    background: url(data:image/png;base64,) no-repeat left center;
    background-size: 30rpx 40rpx;
}

.limit {
    font-size: 24rpx;
    color: #888;
    line-height: 34rpx;
}

.value {
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    width: 30%;
    font-size: 56rpx;
    font-weight: 200;
    padding-right: 10rpx;
    -webkit-box-flex: 0;
    -ms-flex-positive: 0;
    flex-grow: 0;
}

.value text {
    width: 100%;
    text-align: right;
}

.gray {
    color: #888;
}

.shop button {
    font-size: 22rpx;
    color: #888;
    line-height: 22rpx;
    width: 186rpx;
    text-align: center;
    padding: 14rpx;
    border-radius: 10rpx;
    margin: 8rpx 0;
    background: #fff;
}
